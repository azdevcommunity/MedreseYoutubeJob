{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Debug",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}",
          "standardErrorFromLevel": "Error"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProperty"
    ],
    "Properties": {
      "Application": "YoutubeApiSynchronize",
      "Environment": "Development"
    }
  },
  "AllowedHosts": "*",
  "YoutubeConfig": {
    "DeveloperKey": "AIzaSyDMvdiGC4mEeKBMnFr1ZLt_ZwXbr_aVIzM",
    "ChannelID": "UC1B8clCxtmb-bzCDdxmVPDA"
  }
}
